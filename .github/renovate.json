{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",

	"customManagers": [
		{
			"customType": "jsonata",
			"fileFormat": "json",
			"managerFilePatterns": [
				"/^.*/upstream.json$/"
			],
			"matchStrings": [
				"{ \"depName\": name, \"datasource\": datasource, \"currentValue\": version }"
			]
		}
	]
}
